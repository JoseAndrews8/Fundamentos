#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    float a, b, c, D, xr, xi;

    cout << "Ingresar los valores de a, b, c (con espacio): ";
    cin >> a >> b >> c;
 
    D = b * b - 4 * a * c;
    
    if ( D < 0 ) {
        xr = - b / ( 2 * a );
        xi = sqrt( - D ) / ( 2 * a );
        
        cout << "X1 = " << xr << "+" << xi << "i" << endl;
        cout << "X2 = " << xr << "-" << xi << "i" <<endl;
    } else {
            cout << "No hay solucion imaginaria ";
        }
    return 0;
}
